.contact__up-btn
  width 50px
  height 50px
  border 0
  padding 0
  border-radius 50%
  background rgba(61, 201, 179, 0.5)
  display block
  margin 0 auto
  color #fff
  font-size 18px
  line-height 46px
  text-align center
  position absolute
  top -26px
  left 0
  right 0
  margin-left auto
  margin-right auto
  cursor pointer
  transition all 0.5s
  &:hover, &:focus
    background rgba(61, 201, 179, 1)